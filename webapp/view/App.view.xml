<mvc:View
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	controllerName="sap.ui.sap_walkthrough.controller.App"
	displayBlock="true"
>
	<!-- <Text text="Hello UI5 again!" /> -->
	<Shell>
		<App class="inlineMyAppDemo">
			<pages>
				<Page title="{i18n>homePageTitle}">
					<headerContent>
						<Button 
							icon="sap-icon://hello-world"
							press=".onOpenDialog" />
					</headerContent>
					<content>
						<mvc:XMLView viewName="sap.ui.sap_walkthrough.view.HelloPanel" />
					</content>
				</Page>
			</pages>
		</App>
	</Shell>
</mvc:View>