<mvc:View
	controllerName="sap.ui.sap_walkthrough.controller.App"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:c="sap.ui.core"
	displayBlock="true"
	height="100%"
	busyIndicatorDelay="0"
>
	<Page title="{i18n>homePageTitle}">
		<headerContent>
			<Button 
				icon="sap-icon://hello-world"
				press=".onOpenDialog"/>
		</headerContent>
		<content>
			<mvc:XMLView viewName="sap.ui.sap_walkthrough.view.HelloPanel"  />
			<mvc:XMLView viewName="sap.ui.sap_walkthrough.view.InvoiceList" />
		</content>
	</Page>
</mvc:View>